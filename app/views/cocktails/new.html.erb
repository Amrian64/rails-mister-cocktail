<div class="new-background-img">
  <div class="container">
    <%= simple_form_for(@cocktail) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.input :photo %>
      <%= f.input :photo_cache, as: :hidden %>
      <%= f.submit %>
    <% end %>

    <%= link_to "Back" , cocktails_path%>
  </div>
</div>



